    "galleryBanner": {
        "color": "#e3f4ff",
        "theme": "light"
    },
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "tslint -c tslint.json 'src/**/*.ts'"
    },
